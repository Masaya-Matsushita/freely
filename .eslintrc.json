{
  "plugins": ["tailwindcss", "import-access"],
  "extends": [
    "next/core-web-vitals",
    "plugin:import/recommended",
    "plugin:import/warnings",
    "plugin:tailwindcss/recommended",
    "prettier"
  ],
  "rules": {
    "no-restricted-imports": ["error", { "patterns": ["../"] }],
    "import/order": [
      "error",
      {
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "import-access/jsdoc": ["error"]
  }
}
